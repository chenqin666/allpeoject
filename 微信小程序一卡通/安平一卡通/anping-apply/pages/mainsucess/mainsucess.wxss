.totips{
    top: 32%;
    border-radius: 15px;
    width: 70%;
    text-align: center;
    height: 320rpx;
    position: fixed;
    left: 16%;

    background-color: #FFFFFF;
    z-index: 11;
    /* top:0;
    right:0;
    left:0;
    bottom:0; */
}
.p1{
    font-size: 32rpx;
    margin: 30rpx auto;
    line-height: 40rpx;
}
.p2{
    width: 80%;
    font-size: 27rpx; 
    margin: 20rpx auto;
}
.p3{
   height: 60rpx;
    margin: 10rpx auto;
}
.p3 input{
    width: 76%;
    height: 60rpx;
    font-size: 30rpx; 
    text-align: left;
    margin: 10rpx auto;
    text-indent: 20rpx;
    border: 1px solid #dedede;

}
.tofooter{
    width: 100%;
    height: 50rpx;
    margin-top: 30rpx;
    border-top: 0.5px solid #dedede;
    display: flex;
    flex-direction: row;
}
.tofooter view{
  flex: 1;
  line-height: 50rpx;
  height:50rpx;
  display: inline-block;
  font-size: 30rpx;
  margin: 10rpx auto;
}
.popLayer{
  background-color: #B3B3B3;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 10;
  -moz-opacity: 0.8;
  opacity:.80;
  filter: alpha(opacity=80);/* 只支持IE6、7、8、9 */
}