.page-add-todo {
  position: fixed;
  width: 100%;
  height: 100%;
  /* background-color: #fff; */
  background: #f9f9f9;
  display: flex;
  flex-direction: column;
}
.content_box {
    width: 100%;
    height: 100%;
    /* overflow: hidden; */
    overflow-y: scroll;
    
}
.cell-box {
    background: white;
    width: 100%;
    height: 500rpx;
    margin: 0 auto;
     padding-top: 35rpx;
    /* margin-top: 25rpx; */
    display: flex;
    flex-direction: column;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    align-items: center;
    justify-content: flex-start;

}
.topimg{
    width: 94%;
    height: 380rpx;
    position: relative;
    /* flex: 1; */
   
}
.topimg .sdf{
    width: 100%;
    height: 100%;
}
.topimg img{
    width: 100%;
    height: 100%;
}
.topimg .footers{
    width: 100%;
    background: rgb(247,97,96);
    height: 50rpx;
    position: absolute;
    bottom: 0;
    border-radius:0 0 28rpx 28rpx;
    /* border-bottom-right-radius:0.5em;
    border-bottom-left-radius:0.5em; */
}
.hea{
    width: 100%;
    margin-top: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: absolute;
    top: 0;
    display: flex;
}
.hea view{
    /* display: inline-block; */
    flex: 1;
    color: white;
    font-weight: normal;
    font-size: 27rpx;
    margin-left: 30rpx;
}
.hea view image{
    width: 35rpx;
    height: 35rpx;
    margin: -7rpx 10rpx;
}
.center{
    width: 100%;
    text-align: center;
    color: white;
    position: absolute;
    top: 130rpx;
    /* left: 23%; */
    align-items: center;
    -webkit-align-items: center;
}
.pscen{
    text-align: center;
    font-size: 35rpx;
}
.footcen{
    display: inline-block;
    font-size: 26rpx;
    margin-top: 10rpx;
}
.footers view{
    margin: 10rpx 40rpx;
    color: white;
    font-size: 27rpx;
}
.chongzhi{
    /* background: red; */
    margin: 30rpx;
    width: 100%;
    display: flex;
    /* font-size: 0.3rem; */
    margin-top: 40rpx;
    flex-direction: row;
    color: black;
}
.boxs{
    flex: 1;
     /* text-align: center; 
    margin-left: 20rpx;*/
    margin-left: 70rpx;
    margin-right: 20rpx;
}
.boxs text{
  margin-top: -10rpx;
}

.boxs image{
    width: 46rpx;
    height: 46rpx;
    margin: -10rpx 10rpx;
}
.mainall{
    width: 100%;
    font-family: 'PingFangSC-Regular';
    border-top: 0.5px solid #dedede;
    /* height: 300rpx; */
    background: white;
    margin: 30rpx auto;
    display: flex;
    flex-direction: column;
}
.maintop{
    flex: 1;
    height: 100rpx;
    font-size: 33rpx;
    border-bottom: 0.5px solid #dedede;
}
.maintop .aimg{
    width: 35rpx;
    height: 35rpx;
    float: left;
    margin: 30rpx  40rpx;
}
.maintop .mainright{
    width: 80%;
    margin: 25rpx  auto;
    float: left;
    position: relative;
}
.mainright image{
    width: 20rpx;
    height: 28rpx;
    text-align: right;
    align-items: right;
    -webkit-align-items: right;
    position: absolute;
    right:20rpx;
    margin-top: 9rpx;
}
.spans{
    display: inline-block;
    width: 250rpx;
    margin: -7rpx auto;
}
.bottom_box{
    position: fixed;
    bottom: 30rpx;
    width: 100%;
    margin: 0 auto;
    font-size: 28rpx;
    color: gray;
    text-align: center;
}




 .mint-msgbox {
    top: 45%;
    border-radius: 15px;
    width: 70%;

}
.mint-msgbox-btn{
    font-size: 17px;
}
.mint-msgbox-input input{
    /* height: 20px; */
    width: 90%;
    margin: 0 0.2rem;
    /* place-content: '请输入校园卡密码'; */
    
}
.mint-msgbox-title{
    font-weight: normal;
    font-size:30rpx;
}
.mint-msgbox-message{
    font-size: 0.27rem;
