}


.result_error{
	width: 574rpx;
	height: 86rpx;
	font-family: PingFangSC-Regular;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 60rpx;
	letter-spacing: 0rpx;
	color: #f76160;
