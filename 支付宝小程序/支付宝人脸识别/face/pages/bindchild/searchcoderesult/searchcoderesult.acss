.title{
  width: 300rpx;
	height: 50rpx;
	font-family: PingFangSC-Regular;
	font-size: 44rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 72rpx;
	letter-spacing: 0rpx;
	color: #333333;
  margin: 0 auto;
  margin-top: 141rpx;
  text-align: center;
}

.oneMargin{
  margin-top: 159rpx;
}

.cell{
  margin-left: 126rpx;
  margin-bottom: 30rpx;

}
.cell span{
	font-family: PingFangSC-Regular;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
  height: 60rpx;
	line-height: 60rpx;
	letter-spacing: 0px;
	color: #333333;
}

.cell .code{
	font-family: PingFangSC-Regular;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 72rpx;
	letter-spacing: 0px;
