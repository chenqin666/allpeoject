.page-todos {
 position: fixed;
  width: 100%;
  height: 100%;
  /* background-color: #fff; */
  background: #f9f9f9;
  display: flex;
  flex-direction: column;
}
.chantitie{
   width: 100%;
   position: fixed;
   font-weight: bold;
   top: 0;
    font-size: 'PingFangSC-Regular';
    background-color: #ffffff;
    border-bottom: 0.5px solid #dedede;
    text-align: center;
    font-size: 0.36rem;
    height: 1.2rem;
    line-height: 1.2rem;
}
.chantitie img{
    width: 0.25rem;
    height: 0.15rem;
}
.mainsall{
    font-family: 'PingFangSC-Regular';
    width: 100%;
    margin:0 auto;
    margin-top: 1.6rem;
    background-color: #ffffff;
    border-radius: 0.10rem;
}
.mainsall input{
    text-align: right;
}
.box{
    display: flex;
    height:0.9rem;
    border-bottom: 1px solid #dedede;
    margin: 3px 12px;
    position: relative;
    align-items: center;
}
.box input{
    font-size: 0.3rem;
    width: 100%;
    border: 0;
    padding-left: 1.5rem;
}


.box text{
    height: 1rem;
    line-height: 1rem;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 0.3rem;
    letter-spacing: 0px;
    color: #333333;
    position: absolute;
    left: 0;
}
.rights{
  flex: 1;
text-align: right;
font-size: 0.3rem;
margin-right: 0.2rem;
font-weight: bold;
}
.btn{
    width: 90%;
    height: 0.88rem;
    background-color: #36a7fa;
    font-size: 0.34rem;
    color: #ffffff;
    margin: 0 auto;
    margin-top: 0.54rem;
    border-radius:  0.1rem;
    text-align: center;
    line-height: 0.88rem;
    
}
