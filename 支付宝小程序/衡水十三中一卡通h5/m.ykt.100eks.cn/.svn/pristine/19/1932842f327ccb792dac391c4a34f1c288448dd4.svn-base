.content_box {
    width: 100%;
    height: 100%;
    overflow: hidden;
    overflow-y: scroll;
    
}
.content_boxcar{
    width: 100%;
    /* height: 100%; */
    /* overflow: hidden; */
    overflow-y: scroll;
    margin-top: 0.1rem;
    /* position: fixed;
    bottom: 0.1rem;
    top: 1rem; */
}
.cell-box {
    background: white;
    width: 100%;
    height: 5.6rem;
    margin: 0 auto;
    margin-top: 0.5rem;
    display: flex;
    flex-direction: column;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    align-items: center;
    justify-content: flex-start;

}
.cell-box-card {
    background: white;
    width: 100%;
    /* height: 5.6rem; */
    margin: 0 auto;
    margin-top: 0.3rem;
    display: flex;
    flex-direction: column;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    align-items: center;
    justify-content: flex-start;

}
.cardheaders{
    margin-top: 0.4rem;
    width: 100%;
    height: 0.5rem;
    line-height: 0.5rem;
    display: flex;
    flex-direction: row;
}
.cardheaders span{
    flex: 1;
    margin-left: 0.3rem;
}
.cardheaders .span_1{
    font-size: 0.3rem;
}
.cardheaders .span_2 img{
    width: 0.25rem;
    height: 0.25rem;
    margin-top: -0.03rem
}
.cardheaders .span_2{
    text-align: right;
    font-size: 0.27rem;
    color: #2ab1f3;
    margin-right: 0.3rem;
}
.topimg_card{
    width: 94%;
    height: 3.7rem;
    position: relative;
    /* flex: 1; */
   
}
.topimg_card .sdf{
    width: 100%;
    height: 100%;
}
.topimg_card img{
    width: 100%;
    height: 100%;
}
.topimg_card .footers_card{
    width: 100%;
    height: 0.7rem;
    position: absolute;
    bottom: 0;
    border-radius:0 0 0.3rem 0.3rem;
}
.topimg_card .footers_card p{
    text-align: right;
    margin: 0.05rem 0.4rem;
    color: white;
    font-size: 0.27rem;
}
.topimg{
    width: 94%;
    height: 4.2rem;
    position: relative;
    /* flex: 1; */
   
}
.topimg .sdf{
    width: 100%;
    height: 100%;
}
.topimg img{
    width: 100%;
    height: 100%;
}
.topimg .footers{
    width: 100%;
    background: rgb(247,97,96);
    height: 0.7rem;
    position: absolute;
    bottom: 0;
    border-radius:0 0 0.3rem 0.3rem;
    /* border-bottom-right-radius:0.5em;
    border-bottom-left-radius:0.5em; */
}
.hea{
    width: 100%;
    margin-top: 0.4rem;
    height: 0.5rem;
    line-height: 0.5rem;
    position: absolute;
    top: 0;
    display: flex;
}
.hea span{
    /* display: inline-block; */
    flex: 1;
    color: white;
    font-weight: normal;
    font-size: 0.27rem;
    margin-left: 0.5rem;
}
.hea .spants{
    /* display: inline-block; */
    flex: 1;
    color: white;
    font-weight: normal;
    font-size: 0.27rem;
    margin-right: 0.3rem;
    text-align: right;
}
.hea span img{
    width: 0.4rem;
    height: 0.4rem;
}

.center{
    width: 100%;
    text-align: center;
    color: white;
    position: absolute;
    top: 1.6rem;
    /* left: 23%; */
    align-items: center;
    -webkit-align-items: center;
}

.footers p{
    margin: 0.2rem 0.4rem;
    color: white;
    font-size: 0.27rem;
}
.pscen{
    text-align: center;
    font-size: 0.37rem;
}
.footcen{
    display: inline-block;
    font-size: 0.27rem;
    margin-top: 0.2rem;
}
.chongzhi{
    /* background: red; */
    padding: 0.4rem;
    width: 100%;
    display: flex;
    font-size: 0.3rem;
    margin-top: 0.1rem;
    flex-direction: row;
    color: black;
}
.left{
    flex: 1;
    margin-left: 0.7rem;
    margin-right: 0.2rem;
}
.right{
    flex: 1;
    margin-left: 0.7rem;
    margin-right: 0.2rem;
}
.left img{
    width: 0.48rem;
    height: 0.48rem;;
}
.right img{
    width: 0.49rem;
    height: 0.49rem;;
}
.mainall{
    width: 100%;
    font-family: 'PingFangSC-Regular';
    border-top: 0.5px solid #dedede;
    height: 3.2rem;
    background: white;
    margin: 0.5rem auto;
    display: flex;
    flex-direction: column;
}
.maintop{
    flex: 1;
    height: 1rem;
    font-size: 0.3rem;
    border-bottom: 0.5px solid #dedede;
}
.maintop .aimg{
    width: 0.35rem;
    height: 0.35rem;
    float: left;
    margin: 0.3rem  0.4rem;
}
.maintop .mainright{
    width: 80%;
    margin: 0.2rem  auto;
    float: left;
    position: relative;
}
.spans{
    display: inline-block;
    width: 4rem;
    margin: 0.12rem auto;
}
.line{
    width: 6rem;
    border: 0.5px solid gray;
}
.mainright img{
    width: 0.2rem;
    height: 0.3rem;
    text-align: right;
    align-items: right;
    -webkit-align-items: right;
    position: absolute;
    right:0.2rem;
    margin-top: 0.15rem;
}
