body,html{
    padding-top: 0.68rem;
    background: #f9f9f9;
    padding-bottom: 1rem;
}





.warp{

    width: 100%;

}

/*微信或者其他浏览器打开的提示*/
.broswer_alert_box{
    width: 100%;
    display: flex;
    flex-direction: column;
    -ms-flex-direction: column;
    -webkit-flex-direction: column;


    justify-content: center;
    -webkit-justify-content: center;
}
.broswer_alert_img {
    width: 2.0rem;
    height: 2.0rem;
    margin: 0 auto;
    margin-top: 1.80rem;
}

.broswer_alert_title {
    width: 4.51rem;
    margin: 0 auto;
    font-family: PingFangSC-Regular;
    font-size: 0.36rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.60rem;
    letter-spacing: 0px;
    color: #000000;
    margin-top: 0.60rem;
}



.ermimg{
    width: 2.0rem;
    height: 2.0rem;
    margin: 0 auto;
    margin-top: 0.50rem;
}

.btn{
    width: 6.6rem;
    height: 0.88rem;
    background-color: #2ab1f3;
    font-size: 0.3rem;
    color: #ffffff;
    margin: 0 auto;
    margin-top: 0.6rem;
    border-radius:  0.1rem;
    text-align: center;
    line-height: 0.88rem;
}

