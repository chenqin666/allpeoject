.btn_box {
    width: 100%;
    display: flex;
    flex-direction: row;
    -ms-flex-direction: row;
    -webkit-flex-direction: row;
    justify-content: space-between;
    -webkit-justify-content: space-between;
    align-items: center;
    -webkit-align-items: center;
    position: fixed;
    bottom: 0;
    height: 1.2rem;
    background-color: #fff;
