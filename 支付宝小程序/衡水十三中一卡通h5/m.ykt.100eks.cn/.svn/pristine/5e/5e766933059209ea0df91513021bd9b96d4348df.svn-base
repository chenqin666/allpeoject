.content_box{
    width: 100%;
    height: 100%;
    overflow: hidden;
    overflow-y: scroll;
    position: fixed;
    top:0;
}

.top_box_out{
    width: 100%;
    background-color: #fff;
    border-bottom: solid 1px #dedede;
}
.top_box{
    width: 7.02rem;
    height: 1.84rem;
    margin: 0 auto;
    position: relative;
}



.top_box>.btn_standard{
    width: 1.60rem;
    height: 0.50rem;
    line-height: 0.50rem;
    background-color: #ffffff;
    border-radius: 0.25rem;
    border: solid 1px #2ab1f3;
    font-family: PingFangSC-Regular;
    font-size: 0.26rem;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #2ab1f3;
    text-align: center;
    position: absolute;
    top: 0.4rem;
}


.top_box>.finish_date{
    font-family: PingFangSC-Regular;
    font-size: 0.26rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.30rem;
    letter-spacing: 0px;
    color: #999999;
    position: absolute;
    top: 1.2rem;
    left: 0;
}


.top_box>.alertnote{
    font-family: PingFangSC-Regular;
    font-size: 0.26px;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.40rem;
    letter-spacing: 0px;
    color: #f76260;
    position: absolute;
    top: 1.74rem;
    left: 0;
}


/*审核等待中样式*/
.status_wait_box{
    width: 100%;
    background-color: #f6f6f6;
    border-bottom: solid 1px #dedede;
}
.status_wait{
    width: 7.02rem;
    height: 3.28rem;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    -ms-flex-direction: column;
    -webkit-flex-direction: column;
    justify-content: center;
    -webkit-justify-content: center;
    align-items: center;
    -webkit-align-items: center;
}



.status_wait>.tag{
    width: 1.0rem;
    height: 1.0rem;
    background: url("../img/icon_waiting.png") no-repeat;
    background-size: cover;
}
.status_wait>.title{
    width: 100%;
    font-family: PingFangSC-Regular;
    font-size: 0.32rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.30rem;
    letter-spacing: 0px;
    color: #333333;
    text-align: center;
    margin-top: 0.30rem;

}


.status_wait>.topalert{
    width: 100%;
    font-family: PingFangSC-Regular;
    font-size: 0.26rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.40rem;
    letter-spacing: 0px;
    color: #f76260;
    text-align: center;
    margin-top: 0.20rem;
}


/*审核不通过样式*/


