    line-height: 0.40rem;
    letter-spacing: 0px;
    color: #f76260;
    text-align: center;
    margin-top: 0.20rem;
}


.btn{
    width: 6.6rem;
    height: 0.88rem;
    background-color: #36a7fa;
    font-size: 0.3rem;
    color: #ffffff;
    margin: 0 auto;
    margin-top: 0.4rem;
    border-radius:  0.44rem;
    text-align: center;
    line-height: 0.88rem;
}


/*内容头部标题*/
.type_title_box{
    width: 100%;
    height: 0.90rem;
    line-height: 0.90rem;
    background-color: #ffffff;
    border-bottom: solid 1px #dedede;
    display: flex;
    flex-direction: row;
    -webkit-flex-direction: row;
    -ms-flex-direction: row;
    align-items: center;
    -webkit-align-items: center;

}


.type_title{
    width: 7.02rem;
    margin: 0 auto;
    border-left: solid 3px #2ab1f3;
    font-family: PingFangSC-Regular;
    font-size: 0.32rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.30rem;
    letter-spacing: 0px;
    color: #333333;
    padding-left: 0.1rem;
    padding-top:1px;
}


.score_img_box {
    background-color: #fff;
    padding-left: 0.24rem;
    padding-bottom: 0.3rem;
    border-bottom: 0.5px solid #dedede;
}

.score_img{
    width: 2.0rem;

}




