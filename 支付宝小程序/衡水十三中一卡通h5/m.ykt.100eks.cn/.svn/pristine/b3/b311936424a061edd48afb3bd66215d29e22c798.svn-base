.box{
    display: flex;
    width: 92%;
    height:1.1rem;
    /* padding: 1px; */
    border-bottom: 1px solid #dedede;
    margin: 3px auto;
    /* overflow: hidden; */
    position: relative;
    align-items: center;
}

.boxalert{
    height:0.8rem ;
    border-bottom: 1px solid #2ab1f3;
}
.box input{
    font-size: 0.3rem;
    width: 100%;
    /* height:1rem;
    line-height: 1rem; */
    border: 0;
    padding-left: 0.60rem;
}

.box .alert{
    width: 100%;
    font-family: PingFangSC-Regular;
    font-size: 0.36rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.85rem;
    letter-spacing: 0px;
    color: red;
    text-align: left;
    float: left;
}

.box span{
    height: 1rem;
    line-height: 1rem;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 0.30rem;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #333333;
    position: absolute;
    left: 0;
}

.box .fr{
    width: 1.8rem;
    right: 0;
    left:inherit;
    color: #36a7fa;
}

.fr img{
    width: 100%;
    height:100%;
}

.box .fr s{
    position: absolute;
    right:0;
    top:50%;
    width: 1px;
    display: block;
    height:0.24rem;
    background: #dedede;
    margin-top: -0.12rem;
}
